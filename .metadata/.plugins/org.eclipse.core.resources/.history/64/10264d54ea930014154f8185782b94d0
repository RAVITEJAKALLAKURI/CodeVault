public class LineUp {
 public static void main(String[] args) {
 double d = 12.34512;
 // insert code here

 
// System.out.printf("|%7d| \n", d);
 System.out.printf("|%7f| \n", d);
 //System.out.printf("|%3.7d| \n", d);
 System.out.printf("|%3.7f| \n", d);
// System.out.printf("|%7.3d| \n", d);
 System.out.printf("%f ", d);
 }
}